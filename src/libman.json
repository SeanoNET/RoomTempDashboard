{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@aspnet/signalr@1",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@2.7.3",
      "destination": "wwwroot/lib/Chartjs/",
      "files": [
        "Chart.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "moment.js@2.24.0",
      "destination": "wwwroot/lib/moment.js/",
      "files": [
        "moment.min.js",
        "moment.js"
      ]
    }
  ]
}